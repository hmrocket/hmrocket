<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/secondaryProgress">
        <rotate
            android:fromDegrees="277"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="277">
            <shape
                android:innerRadiusRatio="3"
                android:shape="ring"
                android:thicknessRatio="14.0"
                android:useLevel="true">

                <solid android:color="@color/progress_overall_background" />
            </shape>
        </rotate>
    </item>
    <item android:id="@android:id/progress">
        <rotate
            android:fromDegrees="270"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="270">
            <shape
                android:innerRadiusRatio="3"
                android:shape="ring"
                android:thicknessRatio="14.0"
                android:useLevel="true">

                <corners android:radius="10dp" />
                <solid android:color="@color/progress_overall_orange" />
                <stroke
                    android:width="1dip"
                    android:color="@color/progress_overall_orange" />
            </shape>
        </rotate>
    </item>
</layer-list>